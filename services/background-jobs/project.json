{
  "root": "services/background-jobs",
  "projectType": "application",
  "sourceRoot": "services/background-jobs/src",
  "targets": {
    "build": {
      "executor": "./tools/executors/workspace:run-command",
      "options": {
        "cwd": "services/background-jobs",
        "color": true,
        "command": "sls package",
        "exitCommand": "echo \"we out\""
      }
    },
    "serve": {
      "executor": "./tools/executors/workspace:run-command",
      "options": {
        "cwd": "services/background-jobs",
        "color": true,
        "parallel": true,
        "command": "docker-compose up",
        "exitCommand": "docker-compose stop"
      }
    },
    "deploy": {
      "executor": "./tools/executors/workspace:run-command",
      "options": {
        "cwd": "services/background-jobs",
        "color": true,
        "command": "sls deploy"
      }
    },
    "remove": {
      "executor": "./tools/executors/workspace:run-command",
      "options": {
        "cwd": "services/background-jobs",
        "color": true,
        "command": "sls remove"
      }
    },
    "analyze": {
      "executor": "./tools/executors/workspace:run-command",
      "options": {
        "cwd": "services/background-jobs",
        "color": true,
        "command": "sls package --analyze {args.function}"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["services/background-jobs/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/services/background-jobs"],
      "options": {
        "jestConfig": "services/background-jobs/jest.config.js",
        "passWithNoTests": true
      }
    },
    "console": {
      "executor": "./tools/executors/workspace:run-command",
      "options": {
        "cwd": "services/public-api",
        "color": true,
        "command": "node --experimental-repl-await ./console.js"
      }
    }
  },
  "tags": ["service"]
}
